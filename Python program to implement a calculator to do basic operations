defadd(x,y):
print(x+y)
def subtract(x,y):
print(x-y)
def multiply(x.y):
print(x*y)
def divide(x,y):
print(x/y)
print("Enter two numbers")
nl=input()
n2=input()
print("Enter the operation +,-,*,/ ")
op=input()
if op=="+"
add(int(nl),int(n2))
elif op=="-"
subtract(int(nl),int(n2))
elif op=="*"
multiply(int(nl),int(n2))
elif op=="":
divide(int(nl),int(n2))
else:
print(" Invalid entry ")



